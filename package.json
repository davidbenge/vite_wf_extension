{
  "name": "703SalmonCanidae",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "dependencies": {
    "@adobe/aio-lib-core-logging": "^3.0.2",
    "@adobe/aio-lib-state": "^5.1.0",
    "@adobe/exc-app": "^0.2.21",
    "@adobe/react-spectrum": "^3.4.0",
    "@adobe/uix-guest": "^0.10.0",
    "@adobe/workfront-ui-ext-tpl": "^0.0.7",
    "@react-spectrum/list": "^3.0.0-rc.0",
    "@spectrum-icons/workflow": "^4.2.20",
    "@types/swagger-schema-official": "^2.0.25",
    "chalk": "^4",
    "node-fetch": "^2.6.0",
    "node-html-parser": "^5.4.2-0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-router-dom": "^6.20.0",
    "swagger-schema-official": "^2.0.0-bab6bed"
  },
  "devDependencies": {
    "@openwhisk/wskdebug": "^1.3.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-watch": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "vite-plugin-node-polyfills": "^0.19.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "scripts": {
    "start": "npm run dev:full",
    "dev": "vite",
    "dev:watch": "concurrently \"tsc --watch -p src/workfront-ui-1/web-src/tsconfig.json\" \"vite --force\"",
    "dev:full": "concurrently -n \"vite,tsc,lint,test\" -c \"green,blue,yellow,magenta\" \"npm run dev\" \"npm run typecheck:watch\" \"npm run lint:watch\" \"npm run test:watch\"",
    "aio:dev": "npm run transform:yaml-to-json && concurrently -n \"vite,tsc,lint,test,meta\" -c \"green,blue,yellow,magenta,cyan\" \"npm run dev\" \"npm run typecheck:watch\" \"npm run lint:watch\" \"npm run test:watch\" \"npm run watch:metadata\"",
    "aio:build:actions": "rimraf src/workfront-ui-1/actions && tsc -p tsconfig.actions.json && aio app build --no-content-hash --no-force-build --no-web-assets",
    "aio:deploy:actions": "aio app deploy --no-build --no-content-hash --no-web-assets",
    "build": "tsc && vite build",
    "build:watch": "concurrently \"tsc --watch -p src/workfront-ui-1/web-src/tsconfig.json\" \"vite build --watch\"",
    "preview": "vite preview",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "jest --collectCoverage=false --testRegex ./e2e",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:watch": "esw -w --cache --ext .ts,.tsx,.js,.jsx src test actions",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "transform:yaml-to-json": "node node_modules/@adobe/uix-guest/scripts/generate-metadata.js",
    "watch:metadata": "nodemon --watch \"src/**/*.yaml\" --ext yaml,yml --exec \"npm run transform:yaml-to-json\"",
    "clean": "rimraf dist .eslintcache coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch -p src/workfront-ui-1/web-src/tsconfig.json",
    "validate": "concurrently \"npm run typecheck\" \"npm run lint\" \"npm run test\" \"npm run format:check\"",
    "prepare": "npm run validate"
  },
  "description": "test the port from js to ts and to vite and off parcel",
  "aio-app-builder-templates": [
    "@adobe/workfront-ui-ext-tpl"
  ]
}
